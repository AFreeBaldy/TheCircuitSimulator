<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root id="load-project" styleClass="root" stylesheets="@../styles/style.css" type="Pane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.capedbaldy.thecircuitsimulator.controllers.LoadProjectController">
   <children>
      <HBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0">
         <children>
            <VBox alignment="CENTER" spacing="41.0">
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="LOAD PROJECT" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ListView fx:id="listView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="670.0" styleClass="list-view-1" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Pane fx:id="backButton">
                           <children>
                              <fx:include source="button-1.fxml" />
                           </children>
                        </Pane>
                        <Pane fx:id="deleteButton" layoutX="10.0" layoutY="10.0">
                           <children>
                              <fx:include source="button-1.fxml" />
                           </children>
                        </Pane>
                        <Pane fx:id="renameButton" layoutX="170.0" layoutY="10.0">
                           <children>
                              <fx:include source="button-1.fxml" />
                           </children>
                        </Pane>
                        <Pane fx:id="copyButton" layoutX="330.0" layoutY="10.0">
                           <children>
                              <fx:include source="button-1.fxml" />
                           </children>
                        </Pane>
                        <Pane fx:id="loadButton" layoutX="490.0" layoutY="10.0">
                           <children>
                              <fx:include source="button-1.fxml" />
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</fx:root>
